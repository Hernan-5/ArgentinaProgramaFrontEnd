  }
  .about-section {
    margin-top: 0px !important;
  }
  .parallax .parallax-content .count-item {
    height: auto;
    padding-top: 20px;
    padding-bottom: 20px;
  }
  .parallax .parallax-content .count-item:hover strong {
    margin-top: 0px;
  }
  .parallax .parallax-content .count-item:before {
